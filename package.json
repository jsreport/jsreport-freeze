{
  "name": "jsreport-freeze",
  "version": "1.0.1",
  "author": {
    "name": "Jan Blaha"
  },
  "description": "jsreport extension allowing to freeze editing of templates",
  "homepage": "https://github.com/jsreport/jsreport-freeze",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-freeze.git"
  },
  "scripts": {
    "build": "jsreport-studio-build",
    "start": "jsreport-studio-start --httpPort 5488",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "devDependencies": {
    "babel-eslint": "6.0.4",
    "eslint": "2.11.1",
    "eslint-config-standard-jsx": "1.2.1",
    "eslint-plugin-babel": "3.2.0",
    "jsreport-studio-dev": "1.0.0",
    "standard": "7.1.2"
  },
  "dependencies": {
    "in-publish": "2.0.0",
    "jsreport": "1.10.0"
  },
  "keywords": [
    "jsreport"
  ],
  "license": "MIT",
  "maintainers": [
    {
      "name": "pofider",
      "email": "jan.blaha@hotmail.com"
    }
  ],
  "files": [
    "lib",
    "index.js",
    "jsreport.config.js",
    "studio"
  ]
}
